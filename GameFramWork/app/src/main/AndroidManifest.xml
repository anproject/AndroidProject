<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gameframework">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/supermario"
        android:label="@string/app_name"
        android:roundIcon="@drawable/supermario"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.GameView.MainActivity"></activity>
        <activity android:name="org.GameView.IntroActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>